{
  "name": "creditcard.js",
  "version": "2.1.3",
  "description": "A simple library for credit-card validation in JavaScript",
  "main": "src/creditcard.js",
  "repository": "ContaAzul/creditcard.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "npm run check && npm run build:min",
    "build:watch": "rollup -c -w",
    "build:min": "rollup -c --environment BUILD:production",
    "lint": "eslint src specs",
    "test": "karma start --single-run",
    "test:watch": "karma start --watch",
    "check": "npm run test && npm run lint",
    "prepublish": "npm run build"
  },
  "keywords": [
    "credit",
    "card",
    "money",
    "validator"
  ],
  "author": "@ContaAzul",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.9.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-standard": "^3.0.1",
    "jasmine-core": "^2.8.0",
    "karma": "^1.7.1",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-rollup-preprocessor": "^5.1.1",
    "phantomjs-prebuilt": "^2.1.15",
    "rollup": "^0.56.5",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-uglify": "^3.0.0"
  }
}
